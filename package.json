    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0"
  },
  "babel": {
    "presets": [
      "./config/babelrc.js"
    ]
  },
  "eslintConfig": {
    "extends": "./config/eslintrc.js"
  },
  "stylelint": {
    "extends": "./config/.stylelintrc.json"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss,css}": [
