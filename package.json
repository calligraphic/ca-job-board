    "file-loader": "1.1.5",
    "flow-bin": "^0.61.0",
    "fs-extra": "3.0.1",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "2.29.0",
    "husky": "^0.14.3",
    "jest": "20.0.4",
