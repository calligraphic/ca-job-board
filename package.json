  "version": "0.1.0",
  "license": "MIT",
  "description": "A Job Board implemented in React",
  "author": "Kevin Brown",
  "keywords": [
    "redux",
    "orm",
    "redux-orm"
  ],
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-node": "1.1.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-react-jsx-self": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "*",
    "babel-runtime": "6.26.0",
    "bootstrap-sass": "^3.3.7",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "^0.61.0",
    "fs-extra": "3.0.1",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "2.29.0",
    "husky": "^0.14.3",
    "jest": "20.0.4",
    "lint-staged": "^6.0.0",
    "lodash": "^4.17.4",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "^1.9.2",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-html-parser": "^2.0.2",
    "react-pure-render": "*",
    "react-redux": "^5.0.6",
    "react-router-redux": "^4.0.8",
    "redux": "*",
    "redux-logger": "*",
    "redux-orm": "^0.9.4",
    "redux-orm-proptypes": "calligraphic/redux-orm-proptypes",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "sass-loader": "^6.0.6",
    "source-map-explorer": "^1.5.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-enzyme": "^4.0.1",
    "react-test-renderer": "^16.2.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "build-css": "node-sass-chokidar --include-path ./src/styles/ --include-path ./node_modules/bootstrap-sass/assets src/ -o public",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles/ --include-path ./node_modules/bootstrap-sass/assets src/ -o public --watch --recursive",
    "start-js": "node scripts/start.js",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "node scripts/build.js",
    "build": "npm-run-all build-css build-js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "flow": "flow",
    "test": "node scripts/test.js --env=jsdom",
    "test:debug": "node scripts/test.js --inspect-brk test --runInBand --env=jsdom",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "babel": {
    "presets": [
      "./config/babelrc.js"
    ]
  },
  "eslintConfig": {
    "extends": "./config/eslintrc.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calligraphic/ca-job-board"
  },
  "bugs": {
    "url": "https://github.com/calligraphic/ca-job-board/issues"
  },
  "homepage": "https://calligraphic.github.io/ca-job-board",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  }
}
