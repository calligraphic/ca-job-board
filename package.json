{
  "name": "ca-job-board",
  "version": "0.1.0",
  "license": "MIT",
  "description": "A Job Board implemented in React",
  "author": "Kevin Brown",
  "keywords": [
    "redux",
    "orm",
    "redux-orm"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "css:lint": "stylelint '**/*.scss'; exit 0",
    "css:build": "node-sass-chokidar --include-path ./src/styles/ src/ -o public",
    "css:watch": "npm run css:build && node-sass-chokidar --include-path ./src/styles/ src/ -o public --watch --recursive",
    "lint": "eslint --config config/eslintrc.js src",
    "start:script": "node scripts/start.js",
    "start": "npm-run-all -p css:watch start:script",
    "build:script": "node scripts/build.js",
    "build": "npm-run-all css:build build:script",
    "test:script": "node scripts/test.js --env=jsdom",
    "test:flow": "flow",
    "test:coverage": "npm run test:script --coverage",
    "test:debug": "npm run test:script --inspect-brk test --runInBand",
    "pretest": "npm run css:lint",
    "posttest": "npm run format",
    "test": "npm run test:script",
    "publish": "gh-pages -b master -d build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "format": "prettier --ignore-path ./config/.prettierignore --config ./config/.prettierrc --write '**/*.{js,jsx,scss,css}'",
    "security": "nsp check",
    "storybook": "start-storybook -p 9001 -s ./public -c ./config/.storybook",
    "storybook:build": "build-storybook -c ./config/.storybook -o ./docs/storybook",
    "flow:add": "flow-typed install --libdefDir './lib/flow'",
    "flow:start": "flow start",
    "flow:stop": "flow stop",
    "flow:suggest": "flow suggest"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-solid": "^5.0.10",
    "@fortawesome/react-fontawesome": "0.0.18",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
